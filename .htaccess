
RewriteEngine On
RewriteRule ^/?(search)/?$ api/DictionaryApi.php

RewriteRule ^/?(blog)/?$ blog.php
RewriteRule ^/?(blog)/([^/]+)?$ blog.php?category=$2
RewriteRule ^/?(blog)/(tag)/([^/]+)?$ blog.php?tag=$3
RewriteRule ^/?(blog)/([^/]+)/([^/]+)(/.+)?$ blog-post.php?category=$2&item=$3

Options All -Indexes

